{
  "name": "@reason-native-web/morph",
  "version": "0.0.2",
  "esy": {
    "build": "dune build --only-packages=morph,morph_client,morph_core",
    "buildEnv": {
      "ODOC_SYNTAX": "re"
    }
  },
  "scripts": {
    "format": "dune build @fmt --auto-promote",
    "examples": "esy b dune build @examples",
    "examples-path": "esy echo #{self.target_dir}/default/examples",
    "docs": "dune build @doc --only-packages=morph,morph_client,morph_core",
    "docs-path": "esy echo #{self.target_dir}/default/_doc/_html",
    "docs-open": "esy oopen #{self.target_dir}/default/_doc/_html/index.html"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@opam/h2-lwt-unix": "*",
    "@opam/h2-lwt": "*",
    "@opam/h2": "*",
    "@opam/httpaf-lwt-unix": "anmonteiro/httpaf:httpaf-lwt-unix.opam#5dff1b4",
    "@opam/httpaf-lwt": "anmonteiro/httpaf:httpaf-lwt.opam#5dff1b4",
    "@opam/httpaf": "anmonteiro/httpaf:httpaf.opam#5dff1b4",
    "@opam/containers": "*",
    "@opam/uri": "*",
    "@opam/lwt_ssl": "*",
    "@opam/lwt": "*",
    "@opam/ssl": "*",
    "@opam/logs": "*",
    "@opam/fmt": "*",
    "@opam/opium_core": "rgrinberg/opium:opium_core.opam",
    "@opam/hmap": "*",
    "ocaml": "<4.8.0",
    "@esy-ocaml/reason": "*"
  },
  "resolutions": {
    "@opam/httpaf-lwt-unix": "anmonteiro/httpaf:httpaf-lwt-unix.opam#5dff1b4",
    "@opam/httpaf-lwt": "anmonteiro/httpaf:httpaf-lwt.opam#5dff1b4",
    "@opam/httpaf": "anmonteiro/httpaf:httpaf.opam#5dff1b4"
  },
  "devDependencies": {
    "@opam/mtime": "*",
    "@opam/routes": "*",
    "@opam/odoc": "*",
    "@reason-native-web/oopen": "*"
  }
}
